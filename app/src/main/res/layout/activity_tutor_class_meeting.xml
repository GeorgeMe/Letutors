<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.TutorClassMeetingActivity">
    <include layout="@layout/bar_for_class_meeting"/>
     <FrameLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent">
         <ScrollView
             android:layout_width="match_parent"
             android:layout_height="match_parent">
             <LinearLayout
                 android:orientation="vertical"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent">
                 <RelativeLayout
                     android:id="@+id/class_meeting_ke_mu_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="科目"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_ke_mu"
                         android:text="高中英语"
                         android:textSize="14sp"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_shen_fen_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="教师身份"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_shen_fen"
                         android:text="在校大学生"
                         android:textSize="14sp"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                 android:background="#c1c1c1"
                 android:layout_width="match_parent"
                 android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_xing_bie_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="性别"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_xing_bie"
                         android:text="男"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                 android:background="#c1c1c1"
                 android:layout_width="match_parent"
                 android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_jia_ge_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="价格"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_jia_ge"
                         android:text="40￥"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_shi_jian_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="授课时间"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_shi_jian"
                         android:text="2016-1-15 14：30"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_di_dian_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="地点"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_di_dian"
                         android:text="重庆市渝北区双凤桥街道翔宇路15号"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>

                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_fang_shi_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="授课方式"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_fang_shi"
                         android:text="一对一"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
                 <RelativeLayout
                     android:id="@+id/class_meeting_hao_ping_r"
                     android:paddingLeft="40dp"
                     android:paddingRight="40dp"
                     android:paddingTop="10dp"
                     android:paddingBottom="10dp"
                     android:background="#FFF"
                     android:gravity="center_vertical"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                     <TextView
                         android:text="好评"
                         android:textSize="14sp"
                         android:layout_alignParentLeft="true"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content" />
                     <TextView
                         android:id="@+id/class_meeting_hao_ping"
                         android:text="80%"
                         android:textSize="14sp"
                         android:layout_alignParentRight="true"
                         android:layout_width="wrap_content"
                         android:drawableRight="@drawable/aliwx_arrow"
                         android:layout_height="wrap_content" />
                 </RelativeLayout>
                 <View
                     android:background="#c1c1c1"
                     android:layout_width="match_parent"
                     android:layout_height="1dp"/>
             </LinearLayout>
         </ScrollView>
         <Button
             android:id="@+id/class_meeting_ok"
             android:layout_marginLeft="10dp"
             android:layout_marginRight="10dp"
             android:layout_marginBottom="25dp"
             android:textSize="14sp"
             android:padding="10dp"
             android:text="确定"
             android:layout_gravity="bottom"
             android:layout_width="match_parent"
             android:layout_height="wrap_content" />
     </FrameLayout>

</LinearLayout>
