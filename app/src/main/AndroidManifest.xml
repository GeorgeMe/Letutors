<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dmd.letutors">

    <application
        android:name=".TutorApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:process="com.dmd.letutors.application"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="@string/umeng_appkey" />
        <meta-data
            android:name="APP_KEY"
            android:value="@string/app_key" />

        <activity
            android:name=".ui.activity.TutorMainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.TutorSplashActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.TutorLoginActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".ui.activity.TutorRegisterActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.TutorTeacherDetailActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:screenOrientation="portrait" />
        <activity android:name=".im.uicustom.ChattingFragmentActivity" />
        <activity android:name=".ui.activity.TutorClassMeetingActivity" />
        <activity android:name=".ui.activity.TutorWalletActivity" />
        <activity android:name=".ui.activity.TutorOrderActivity" />
        <activity android:name=".ui.activity.TutorTransactionDetailActivity" />
        <activity android:name=".ui.activity.TutorEvaluationActivity" />
        <activity android:name=".ui.activity.TutorModifyDataActivity" />
        <activity android:name=".ui.activity.TutorNeedsActivity" />
        <activity android:name=".ui.activity.TutorVouchersActivity" />
        <activity android:name=".ui.activity.TutorAboutUsActivity" />
        <activity android:name=".ui.activity.LeadActivity"></activity>
    </application>

</manifest>
